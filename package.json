{
  "name": "vue-traffic-light-chrome-extension",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-watch": "vue-cli-service  --env.NODE_ENV=development build-watch --mode development"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^1.6.1",
    "firebase": "^9.8.1",
    "pinia": "^2.0.14",
    "uuid": "^8.3.2",
    "vite": "^2.5.4",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "eslint": "8",
    "eslint-plugin-vue": "8",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24",
    "vue-eslint-parser": "^9.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es2021": true,
      "webextensions": true
    },
    "extends": [
      "plugin:vue/base",
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/multi-word-component-names": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
